{
  "name": "@amiearth/tracking-link",
  "version": "0.1.3",
  "description": "Helper utilities for generating outbound tracking URLs for supported carriers.",
  "keywords": [
    "tracking",
    "link",
    "dhl",
    "fedex",
    "ups"
  ],
  "author": "Amiearth <amiearth@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/amiearth/tracking-link.git"
  },
  "homepage": "https://github.com/amiearth/tracking-link",
  "bugs": {
    "url": "https://github.com/amiearth/tracking-link/issues"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint . --ext .ts",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rimraf dist coverage",
    "publish:patch": "npm version patch && npm publish --access public",
    "publish:minor": "npm version minor && npm publish --access public",
    "publish:major": "npm version major && npm publish --access public"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vitest/coverage-v8": "^2.1.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript-eslint": "^8.7.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.1"
  }
}
